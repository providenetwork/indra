{
  "name": "indra-v2-node",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.json\" ",
    "start:debug": "nodemon --config ops/nodemon-debug.json",
    "prestart:prod": "rm -rf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config ops/jest.config.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@connext/messaging": "0.0.19",
    "@connext/types": "0.0.19",
    "@counterfactual/contracts": "0.1.15",
    "@counterfactual/firebase-client": "0.0.5",
    "@counterfactual/node": "0.2.59",
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@nestjs/microservices": "6.5.3",
    "@nestjs/platform-express": "6.5.3",
    "@nestjs/typeorm": "6.1.3",
    "class-validator": "0.10.0",
    "dotenv": "8.1.0",
    "ethers": "4.0.33",
    "interval-promise": "1.3.0",
    "nats": "1.3.0",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "typeorm": "0.2.18",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@counterfactual/types": "0.0.37",
    "@nestjs/testing": "6.5.3",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/supertest": "2.0.8",
    "concurrently": "4.1.2",
    "jest": "24.9.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.5.3",
    "wait-on": "3.3.0"
  }
}
